-- Create a book database

CREATE TABLE books
 (id INTEGER PRIMARY KEY AUTOINCREMENT, name TEXT, autor TEXT, year INTEGER, type TEXT);

INSERT INTO TABLE books VALUES 
(“A ciranda das mulheres sabias”, “Clarissa Pinkola Estes”, 2007, “Fiction”),
